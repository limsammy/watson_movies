<p id="notice"><%= notice %></p>

<p>
  <strong>Body:</strong>
  <%= @document.body %>
  <hr>
  <% @document.get_data.each do |tone| %>
    <b><%= tone['tone_name'] %>:</b> <p>%<%= tone['score'] * 100 %></p><br>
  <% end %>
</p>

<%= link_to 'Edit', edit_document_path(@document) %> |
<%= link_to 'Back', documents_path %>
